{
  "name": "hugostrap",
  "version": "0.0.1",
  "private": true,
  "authors": [
    "Nathan Smith <webalkymst@gmail.com>"
  ],
  "description": "hugo + gulp.js + bootstrap 4",
  "keywords": [
    "blog",
    "static",
    "gulp",
    "bootstrap",
    "site"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "vendor",
    "test",
    "tests"
  ],
  "dependencies": {
    "font-awesome": "4.7.0",
    "bootstrap": "4.0.0-alpha.5",
    "animate.css": "3.5.2",
    "jquery": "3.1.1",
    "jquery.counterup": "2.0.5",
    "jquery-parallax": "1.1.3",
    "js-cookie": "1.4.1",
    "waypoints": "2.0.5",
    "gmaps": "v0.4.24",
    "owl.carousel": "2.2.0"
  },
  "overrides": {
    "font-awesome": {
      "main": [
        "less/font-awesome.less",
        "scss/font-awesome.scss",
        "fonts/fontawesome-*.*"
      ]
    },
    "jquery.counterup": {
      "main": [
        "jquery.counterup.js"
      ]
    }
  }
}
